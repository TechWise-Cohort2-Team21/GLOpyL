defina traducir_palabra(palabra: secuencia) -> secuencia:
    si palabra en palabras_clave:
        regresa palabras_clave[palabra]
    si lenguaje_de_programación == "pitón":
        palabras = palabra.dividir("")
        regresa traducir.traducir(" ".unirse(palabras), comenzar=idioma_traducido).texto.reemplazar(" ", "")


